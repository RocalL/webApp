<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:element name="utilisateurs">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="utilisateur" maxOccurs="unbounded" minOccurs="1">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="id" type="xs:int"></xs:element>
							<xs:element name="email" type="xs:string"></xs:element>
							<xs:element name="password"></xs:element>
							<xs:element name="nom" type="xs:string"></xs:element>
							<xs:element name="prenom" type="xs:string"></xs:element>
							<xs:element name="tel" type="xs:int"></xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>